<?xml version="1.0"?>
<routeMapping>
	<routes>
		<add url="" controller="Home" action="Index" />
		<add url="about" controller="Home" action="About" />
		<add url="login" controller="Home" action="Login" />
		<add url="contact/" controller="Feedback" action="Contact" />
		<add url="forums" controller="Forums" action="List" />
		<add url="external/xd_receiver.htm" controller="Base" action="Static">
			<defaults>
				<add key="key" value="FacebookXDReceiver" />
			</defaults>
		</add>
		<add url="users/{id}" controller="Users" action="Detail">
			<constraints>
				<add key="id" value="\d+" />
			</constraints>
		</add>
		<add url="admin/templates/add" controller="Admin" action="AddTemplate" />
		<add url="admin/templates/set" controller="Admin" action="TemplateSetCurrent" />
		<add url="admin/templates/" controller="Admin" action="ListTemplates" />
		<add url="admin/forums/" controller="Forums" action="Manage" />
		<add url="admin/forums/add" controller="Forums" action="Add" />
		<add url="admin/forums/edit" controller="Forums" action="Edit" />
		<add url="admin/" controller="Admin" action="Dashboard" />
		<add url="rss" controller="Forums" action="LatestAllTopics" />
		<add url="topics/pagemore/{id}" controller="Topics" action="PageMore" />
		<add url="topics/pageuntil/{id}" controller="Topics" action="PageUntil" />
		<add url="{forum}/rss" controller="Forums" action="LatestTopics">
			<constraints>
				<add key="forum" value="[\w-]{1,32}" />
			</constraints>
			<defaults>
				<add key="page" value="0" />
				<add key="format" value="Rss" />
			</defaults>
		</add>
		<add url="{forum}/latest/{page}" controller="Forums" action="LatestTopics">
			<constraints>
				<add key="forum" value="[\w-]{1,32}" />
				<add key="page" value="\d+" />
			</constraints>
			<defaults>
				<add key="page" value="0" />
				<add key="format" value="Html" />
			</defaults>
		</add>
		<add url="{forum}/create-topic" controller="Topics" action="Add">
			<constraints>
				<add key="forum" value="[\w-]{1,32}" />
			</constraints>
		</add>
		<add url="{forum}/tags/{tag}/{page}" controller="Tags" action="Detail">
			<constraints>
				<add key="forum" value="[\w-]{1,32}" />
				<add key="page" value="\d+" />
				<add key="tag" value="[\w-]+?" />
			</constraints>
			<defaults>
				<add key="page" value="0" />
			</defaults>
		</add>
		<add url="{forum}/{name}-{id}/reply" controller="Topics" action="Reply">
			<constraints>
				<add key="forum" value="[\w\-]{1,32}" />
				<add key="name" value="[\w-]+?" />
				<add key="id" value="\d+" />
			</constraints>
		</add>
		<add url="{forum}/{name}-{id}/rss" controller="Topics" action="LatestMessages">
			<constraints>
				<add key="forum" value="[\w\-]{1,32}" />
				<add key="name" value="[\w-]+?" />
				<add key="id" value="\d+" />
			</constraints>
		</add>
		<add url="{forum}/{name}-{id}/edit" controller="Topics" action="Edit">
			<constraints>
				<add key="forum" value="[\w\-]{1,32}" />
				<add key="name" value="[\w-]+?" />
				<add key="id" value="\d+" />
			</constraints>
		</add>
		<add url="{forum}/{name}-{id}/{page}" controller="Topics" action="Detail">
			<constraints>
				<add key="forum" value="[\w\-]{1,32}" />
				<add key="name" value="[\w-]+?" />
				<add key="id" value="\d+" />
				<add key="page" value="\d+" />
			</constraints>
			<defaults>
				<add key="page" value="0" />
			</defaults>
		</add>
		<add url="{forum}/{page}" controller="Forums" action="Detail">
			<constraints>
				<add key="forum" value="[\w-]{1,32}" />
				<add key="page" value="\d+" />
			</constraints>
			<defaults>
				<add key="page" value="0" />
			</defaults>
		</add>
		<add url="{id}" controller="Topics" action="ShortUrl">
			<constraints>
				<add key="id" value="\d+" />
			</constraints>
		</add>
	</routes>
</routeMapping>